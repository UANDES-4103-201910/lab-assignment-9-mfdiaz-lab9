<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <% if can? :read, Ticket %>
          <td><%= link_to 'View Tickets', event_tickets_path(event) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
